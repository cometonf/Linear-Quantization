{
  "model": "ResNet18",
  "dataset": "Cifar100",
  "logdir": "log",
  "gpu": "1",
  "save_init": "False",
  "load": {
    "name": null,
    "find_max": "False",
    "find_99th": "False",
    "add_reg_prefix": "False",
    "del_reg_prefix": "False",
    "make_mask": "False",
    "clustering": "False"
  },
  "initializer": {
    "scale": 2.0,
    "mode": "fan_in"
  },
  "activation": "relu",
  "regularizer": {
    "name": "Ridge",
    "lmbd": 0.0002,
    "sub_reg": {
      "relative_lmbd": 0.25,
      "fix_max": "True",
      "sub_ratio": [2,2]
    }
  },
  "quantizer": {
    "name": "linear",
    "BITW": 32,
    "W_opts": {
      "fix_max": "False",
      "max_scale": 1.0,
      "is_Lv": "False",
      "centralized": "False",
      "pruning": "False",
      "threshold_bit": "[2,2]"
    },
    "BITA": 32,
    "BITG": 32
  },
  "optimizer": {
    "name": "Momentum",
    "lr": 0.01,
    "lr_schedule": [[1, 0.1], [82, 0.01], [123, 0.001], [300, 0.0002]],
    "max_epoch": 200
  }
}
