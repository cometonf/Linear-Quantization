{
  "model": "ResNet18",
  "dataset": "Cifar10",
  "logdir": "Cent7LW4A",
  "gpu": "",
  "save_init": "False",
  "load": {
    "name": "centralized.npz",
    "find_max": "False",
    "add_reg_prefix": "False",
    "del_reg_prefix": "False",
    "make_mask": "True",
    "clustering": "False"
  },
  "initializer": {
    "scale": 2.0,
    "mode": "fan_in"
  },
  "activation": "relu1",
  "regularizer": {
    "name": "Ridge",
    "lmbd": 0.0002,
    "sub_reg": {
      "relative_lmbd": 0.25,
      "fix_max": "False",
      "sub_ratio": [2,2]
    }
  },
  "quantizer": {
    "name": "linear",
    "BITW": 7,
    "W_opts": {
      "fix_max": "True",
      "max_scale": 1.0,
      "is_Lv": "True",
      "centralized": "True",
      "pruning": "False",
      "threshold_bit": "[2,2]"
    },
    "BITA": 4,
    "BITG": 32
  },
  "optimizer": {
    "name": "Momentum",
    "lr": 0.01,
    "lr_schedule": [[1, 0.01], [42, 0.001]],
    "max_epoch": 119
  }
}
