{
  "model": "ResNet18",
  "dataset": "Cifar100",
  "logdir": "ResNet18_SQ_W19LA4",
  "gpu": "0",
  "save_init": "False",
  "load": {
    "name": "pretrn_relu1_resnet18.npz",
    "find_max": "True",
    "find_99th": "False",
    "add_reg_prefix": "False",
    "del_reg_prefix": "False",
    "pruning": "False",
    "make_mask": "True",
    "mask_load": "quantized_19l_wr24-8_resnet18.npz",
    "clustering": "False"
  },
  "initializer": {
    "scale": 2.0,
    "mode": "fan_in"
  },
  "activation": "relu1",
  "regularizer": {
    "name": "Ridge",
    "lmbd": 0.0002,
    "sub_reg": {
      "relative_lmbd": 2.0,
      "fix_max": "True",
      "sub_ratio": [2,4]
    }
  },
  "quantizer": {
    "name": "linear",
    "BITW": 19,
    "W_opts": {
      "fix_max": "True",
      "max_scale": 1.0,
      "is_Lv": "True",
      "centralized": "True",
      "stop_grad": "False",
      "pruning": "False",
      "threshold_bit": "[2,4]"
    },
    "BITA": 4,
    "BITG": 32
  },
  "optimizer": {
    "name": "NesterovAG",
    "lr": 0.1,
    "lr_schedule": [[0, 0.1], [101, 0.01], [201, 0.001], [301, 0.0001]],
    "max_epoch": 300
  }
}
