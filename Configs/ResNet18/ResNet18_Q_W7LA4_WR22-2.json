{
  "model": "ResNet18",
  "dataset": "Cifar100",
  "logdir": "ResNet18_Q_W7LA4_WR22-2",
  "gpu": "0",
  "save_init": "False",
  "load": {
    "name": "pretrn_relu1_mul2r_resnet18.npz",
    "find_max": "True",
    "find_99th": "False",
    "add_reg_prefix": "True",
    "del_reg_prefix": "False",
    "pruning": "False",
    "make_mask": "False",
    "mask_load": null,
    "clustering": "False"
  },
  "initializer": {
    "scale": 2.0,
    "mode": "fan_in"
  },
  "activation": "relu1",
  "regularizer": {
    "name": "Weighted_Ridge1",
