      "stop_grad": "False",
      "pruning": "False",
      "threshold_bit": "[2,2]"
    },
    "BITA": 4,
    "BITG": 32,
    "mulR": "2R"
  },
  "optimizer": {
    "name": "NesterovAG",
    "lr": 0.01,
    "lr_schedule": [[0, 0.1], [100, 0.01], [200, 0.001], [300, 0.0001]],
    "max_epoch": 300
  }
