{
  "model": "ResNet18",
  "dataset": "Cifar100",
  "logdir": "ResNet18_WR23",
  "gpu": "0",
  "save_init": "False",
  "load": {
    "name": "pretrn_resnet18.npz",
    "find_max": "True",
    "find_99th": "False",
    "add_reg_prefix": "True",
    "del_reg_prefix": "False",
    "pruning": "False",
    "make_mask": "False",
    "mask_load": null,
    "clustering": "False"
  },
  "initializer": {
    "scale": 2.0,
    "mode": "fan_in"
  },
  "activation": "relu",
  "regularizer": {
    "name": "Weighted_Ridge1",
    "lmbd": 0.0002,
    "sub_reg": {
      "relative_lmbd": 4.0,
      "fix_max": "True",
      "sub_ratio": [2,3]
    }
  },
  "quantizer": {
    "name": "linear",
    "BITW": 32,
    "W_opts": {
      "fix_max": "True",
      "max_scale": 1.0,
      "is_Lv": "True",
      "centralized": "False",
      "stop_grad": "False",
      "pruning": "False",
      "threshold_bit": "[2,3]"
    },
    "BITA": 32,
    "BITG": 32
  },
  "optimizer": {
    "name": "NesterovAG",
    "lr": 0.1,
    "lr_schedule": [[0, 0.1], [101, 0.01], [201, 0.001], [301, 0.0001]],
    "max_epoch": 300
  }
}
